---

import { data } from '@/data/data';
import ListOfNotes from '@/components/ListOfNotes.astro';
import LayoutPages from '@/layouts/LayoutPages.astro';
const { notes } = data;

type Note = {
	id: number,
	title: string,
	content: string,
	href: string,
	important: boolean
}

---

